{
  "name": "cdp-integuru-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for CDP automation with Integuru integration",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "cp -r ../src/lib ./dist/ && cp -r ../extensions ./dist/"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rebrowser-puppeteer": "^24.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/lodash": "^4.14.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "mcp",
    "automation",
    "chrome-devtools-protocol",
    "integuru",
    "api-reverse-engineering",
    "browser-automation"
  ],
  "author": "CDP Automation Team",
  "license": "MIT"
}