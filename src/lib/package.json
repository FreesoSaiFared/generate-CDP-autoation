{
  "name": "cdp-automation-debug-infrastructure",
  "version": "1.0.0",
  "description": "Comprehensive debugging and logging infrastructure for CDP automation platform with GLM-4.5V integration",
  "main": "debug-integration.js",
  "type": "commonjs",
  "scripts": {
    "start": "node debug-integration.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js",
    "lint:fix": "eslint *.js --fix",
    "debug": "node --inspect debug-integration.js",
    "dashboard": "node websocket-server.js",
    "rotate-logs": "node log-rotation.js",
    "generate-report": "node debug-report-generator.js",
    "run-diagnostics": "node diagnostic-tools.js",
    "performance-monitor": "node performance-monitor.js",
    "visual-verify": "node visual-verifier.js",
    "self-debug": "node self-debugging-loop.js",
    "build": "npm run lint && npm run test",
    "dev": "concurrently \"npm run debug\" \"npm run dashboard\"",
    "clean": "rimraf logs/* debug-data/* coverage/*",
    "setup": "npm install && npm run clean && mkdir -p logs debug-data",
    "health-check": "node -e \"const DebugIntegration = require('./debug-integration'); const debug = new DebugIntegration(); debug.getStatus().then(console.log);\""
  },
  "keywords": [
    "debugging",
    "logging",
    "automation",
    "cdp",
    "chrome-devtools-protocol",
    "glm-4.5v",
    "visual-verification",
    "performance-monitoring",
    "error-analysis",
    "self-debugging",
    "regression-testing"
  ],
  "author": "CDP Automation Team",
  "license": "MIT",
  "dependencies": {
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-elasticsearch": "^0.17.4",
    "ws": "^8.14.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "tar": "^6.2.0",
    "zlib": "^1.0.5",
    "crypto": "^1.0.1",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6",
    "commander": "^11.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "rate-limiter-flexible": "^4.0.1",
    "node-cron": "^3.0.3",
    "systeminformation": "^5.21.15",
    "pidusage": "^3.0.2",
    "clinic": "^12.1.0",
    "0x": "^5.7.0",
    "autocannon": "^7.14.0",
    "puppeteer": "^21.5.2",
    "sharp": "^0.32.6",
    "jimp": "^0.22.10",
    "pixelmatch": "^5.3.0",
    "opencv4nodejs": "^5.6.0",
    "tesseract.js": "^5.0.4",
    "axios": "^1.6.2",
    "node-fetch": "^2.7.0",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "redis": "^4.6.10",
    "mongodb": "^6.3.0",
    "sqlite3": "^5.1.6",
    "better-sqlite3": "^9.2.2",
    "pg": "^8.11.3",
    "mysql2": "^3.6.5",
    "elastic-apm-node": "^4.5.0",
    "newrelic": "^11.2.0",
    "datadog": "^0.5.0",
    "prom-client": "^15.1.0",
    "grafana-api": "^1.2.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "openapi3-ts": "^4.0.1",
    "ajv": "^8.12.0",
    "json-schema": "^0.4.0",
    "yaml": "^2.3.4",
    "dotenv": "^16.3.1",
    "config": "^3.3.9",
    "nconf": "^0.12.1",
    "debug": "^4.3.4",
    "semver": "^7.5.4",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "mkdirp": "^3.0.1",
    "ncp": "^2.0.0",
    "cpx": "^1.5.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "ts-jest": "^29.1.1",
    "nyc": "^15.1.0",
    "coveralls": "^3.1.1",
    "codecov": "^3.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cdp-automation/debug-infrastructure.git"
  },
  "bugs": {
    "url": "https://github.com/cdp-automation/debug-infrastructure/issues"
  },
  "homepage": "https://github.com/cdp-automation/debug-infrastructure#readme",
  "config": {
    "debug": {
      "level": "info",
      "colors": true,
      "timestamp": true
    },
    "logging": {
      "level": "info",
      "maxFiles": 30,
      "maxSize": "10MB",
      "datePattern": "YYYY-MM-DD"
    },
    "performance": {
      "samplingInterval": 1000,
      "alertThreshold": 80,
      "memoryThreshold": 512
    },
    "visual": {
      "screenshotFormat": "png",
      "quality": 90,
      "fullPage": true,
      "viewport": {
        "width": 1920,
        "height": 1080
      }
    },
    "selfDebugging": {
      "maxAttempts": 5,
      "strategy": "adaptive",
      "learningEnabled": true
    },
    "dashboard": {
      "port": 3000,
      "host": "localhost",
      "auth": true,
      "ssl": false
    },
    "websocket": {
      "port": 3001,
      "maxConnections": 100,
      "heartbeatInterval": 30000
    },
    "rotation": {
      "interval": "1h",
      "maxSize": "100MB",
      "retentionDays": 30,
      "compression": true
    },
    "reporting": {
      "formats": ["html", "pdf", "json"],
      "includeScreenshots": true,
      "includeMetrics": true,
      "template": "default"
    },
    "glm": {
      "apiEndpoint": "https://api.glm-4.5v.com/v1",
      "maxRetries": 3,
      "timeout": 30000,
      "model": "glm-4.5v-vision"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/test/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/test/**/*.test.js",
      "**/__tests__/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}