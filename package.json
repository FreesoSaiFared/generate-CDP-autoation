{
  "name": "cdp-integuru-automation",
  "version": "1.0.0",
  "description": "Chrome DevTools Protocol automation with Integuru integration for API reverse-engineering",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "node src/test/test-integuru.js",
    "test:integuru": "node src/test/test-integuru.js",
    "test:validate": "node src/test/validate-generated-code.js",
    "install:integuru": "./scripts/install-integuru.sh",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "docs": "jsdoc -c jsdoc.conf.json",
    "build": "npm run lint && npm test",
    "dev": "nodemon src/index.js",
    "debug": "node --inspect src/index.js"
  },
  "keywords": [
    "automation",
    "chrome-devtools-protocol",
    "integuru",
    "api-reverse-engineering",
    "har-analysis",
    "browser-automation",
    "mitmproxy",
    "web-scraping"
  ],
  "author": "CDP Automation Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rebrowser-puppeteer": "^24.8.1",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "yargs": "^17.7.2",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "semver": "^7.5.4",
    "joi": "^17.11.0",
    "json-schema": "^0.4.0",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "jsdoc": "^4.0.2",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/cdp-integuru-automation.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/cdp-integuru-automation/issues"
  },
  "homepage": "https://github.com/your-org/cdp-integuru-automation#readme",
  "config": {
    "integuru": {
      "defaultModel": "gpt-4o",
      "timeout": 30000,
      "tempDir": "./temp",
      "integuruDir": "./Integuru"
    },
    "mitmproxy": {
      "port": 8080,
      "host": "127.0.0.1",
      "harOutput": "./network_requests.har"
    },
    "chrome": {
      "headless": false,
      "userDataDir": "./chrome-user-data",
      "extensions": [
        "./extensions/cdp-stealth"
      ]
    }
  },
  "optionalDependencies": {
    "python-shell": "^5.0.0",
    "node-python": "^0.1.1"
  },
  "peerDependencies": {
    "python3": ">=3.8.0",
    "poetry": ">=1.6.0"
  },
  "files": [
    "src/",
    "scripts/",
    "extensions/",
    "docs/",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "lib": "src/lib",
    "test": "src/test",
    "doc": "docs",
    "man": "man"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your-org"
  }
}